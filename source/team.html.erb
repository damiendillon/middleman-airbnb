<div id="about">
  <div class="about-content">
    <h1>This template is built with <i class="fa fa-heart"></i> by</h1>
    <ul class="list-inline text-center">
      <% data.users.each do |user| %>
    <li>
      <%= image_tag "https://kitt.lewagon.com/placeholder/users/#{user.github_nickname}", class: "img-circle" %>
      <h3><%= user.name.titleize %></h3>
      <p>
        <a href="http://github.com/#{user.github_nickname}">              <i class="fa fa-github"></i>  <%= user.github_nickname %>
        </a>
      </p>
    </li>
  <% end %>
</ul>
  </div>
</div>
</div>
</div>
